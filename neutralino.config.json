{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/myapp-react/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.readDirectory"
  ],
  "modes": {
    "window": {
      "title": "myapp",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/myapp-react/public/logo192.png"
    }
  },
  "cli": {
    "binaryName": "myapp",
    "extensionsPath": "/extensions/",
    "resourcesPath": "/myapp-react/build/",
    "clientLibrary": "/myapp-react/public/__neutralino_globals.js",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2"
  }
}